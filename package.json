{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "Google book api used to search the book and displays the details of the book",
  "main": "server.js",
  "engines": {
    "node": "10.16.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.0",
    "node": "^12.10.0",
    "react-script": "^2.0.5"
  },
  "devDependencies": {},
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravipjoshi/google-book-search.git"
  },
  "keywords": [
    "book",
    "google",
    "book-search",
    "google-book-search",
    "search",
    "save"
  ],
  "author": "Ravi Joshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ravipjoshi/google-book-search/issues"
  },
  "homepage": "https://github.com/ravipjoshi/google-book-search#readme"
}
